vector<int> twoSum(vector<int>& nums, int target) {
    unordered_map<int, int> table;
    vector<int> result;
    for (uint32_t i = 0; i < nums.size(); ++ i) {
        int val = target - nums[i];
        if (table.find(val) != table.end()) {
            result.push_back(i);
            result.push_back(table[val]);
            return result;
        } else {
            table[nums[i]] = i;
        }
    }
    return result;
}
