bool hasCycle(ListNode *head) {
    ListNode* fast = head;
    ListNode* slow = head;
    while (fast && fast->next) {
        fast = fast->next;
        if (fast == slow) {
            return true;
        }
        fast = fast->next;
        if (fast == slow) {
            return true;
        }
        slow = slow->next;
    }
    return false;
}
