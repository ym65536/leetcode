vector<string> findRepeatedDnaSequences(string s) {
    unordered_map<string, int> table;
    for (int i = 0; i + 10 <= s.size(); ++ i) {
        string sub = s.substr(i, 10);
        ++ table[sub];
    }

    vector<string> result;
    for (auto it : table) {
        if (it.second > 1) {
            result.push_back(it.first);
        }
    }

    return result;
}
